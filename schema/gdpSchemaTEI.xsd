         <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:orgName"/>
         <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:persName"/>
      </xs:sequence>
   </xs:group>
   <xs:group name="model.nameLike.agent_sequenceRepeatable">
