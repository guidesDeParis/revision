         <xs:element minOccurs="0" ref="tei:orgName"/>
         <xs:element minOccurs="0" ref="tei:persName"/>
      </xs:sequence>
   </xs:group>
   <xs:group name="model.nameLike.agent_sequenceOptionalRepeatable">
